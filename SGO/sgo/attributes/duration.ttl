@prefix ogit:                   <http://www.purl.org/ogit/> .
@prefix owl:                    <http://www.w3.org/2002/07/owl#> .
@prefix rdfs:                   <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dcterms:                <http://purl.org/dc/terms/> .

ogit:duration
	a owl:DatatypeProperty;
	rdfs:subPropertyOf ogit:Attribute;
	rdfs:label "duration";
	dcterms:description "This field represents a duration of an event, activity, phase, etc. It is recommended to use this attribute in conjunction with durationUnit.";
	dcterms:valid "start=2019-12-10;";
	dcterms:creator "Marek Meyer";
.
