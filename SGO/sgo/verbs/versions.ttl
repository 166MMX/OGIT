	rdfs:subPropertyOf ogit:Verb;
	rdfs:label "versions";
	dcterms:description "The version of a node. This verb is generated internally.";
	# For ranges, see http://dublincore.org/documents/dcmi-period/
	dcterms:valid "start=2017-10-31;";
	dcterms:creator "druss@arago.de";
	dcterms:created "2017-10-31";
	dcterms:modified "2018-05-18";
	ogit:admin-contact "arago GmbH";
	ogit:tech-contact "arago GmbH";
	ogit:history (
		[
			dcterms:identifier "1";
			dcterms:date "2017-10-31";
			dcterms:description "initial";
			dcterms:creator "druss@arago.de";
		]
		[
			dcterms:identifier "2";
			dcterms:date "2018-05-18";
			dcterms:description "Application versioning for HIRO DevPortal & HIRO Desktop";
			dcterms:creator "mosher@klika-tech.com";
		]
	);
	ogit:allowed (
		[
			ogit:from ogit:VersioningData;
			ogit:to ogit.Automation:KnowledgeItem;
		]
		[
			ogit:from ogit:VersioningData;
			ogit:to ogit.Automation:KnowledgeBundle;
		]
		[
			ogit:from ogit.Software:Application;
			ogit:to ogit.Software:Application;
		]
	);
