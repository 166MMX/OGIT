	dcterms:valid "start=2018-01-01;";
	dcterms:creator "arago GmbH";
	dcterms:created "2018-01-01";
	dcterms:modified "2018-01-01";
	ogit:admin-contact "arago GmbH";
	ogit:tech-contact "arago GmbH";
	ogit:scope "NTO";
	ogit:parent ogit:Node;
	ogit:mandatory-attributes (
	);
	ogit:optional-attributes (
          ogit.Auth.Account:displayName
          ogit.Auth.Account:acceptedEmails
          ogit.Auth.Account:defaultScope
          ogit.Auth.Account:defaultTeam
          ogit:firstName
          ogit:lastName
	);
