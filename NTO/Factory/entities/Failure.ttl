@prefix ogit.Factory:   <http://www.purl.org/ogit/Factory/> .
@prefix owl:                    <http://www.w3.org/2002/07/owl#> .
@prefix ogit:                   <http://www.purl.org/ogit/> .
@prefix rdfs:                   <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dcterms:                <http://purl.org/dc/terms/> .

ogit.Factory:Failure
	a rdfs:Class;
	rdfs:subClassOf ogit:Entity;
	rdfs:label "Failure";
	dcterms:description "A failure of a manufacturing plant, machinery component or equipment.";
	dcterms:valid "start=2019-12-10;";
	dcterms:creator "Marek Meyer";
	ogit:scope "NTO";
	ogit:parent ogit:Node;
	ogit:mandatory-attributes (
		
	);
	ogit:optional-attributes (
		ogit:name
		ogit:description
		ogit:subType
		ogit:status
		ogit:severity
	);
	ogit:indexed-attributes (
		
	);
	ogit:allowed (
			[ ogit:affects  ogit.Factory:Component ]
			[ ogit:causes  ogit.Factory:Failure ]
	);
.
