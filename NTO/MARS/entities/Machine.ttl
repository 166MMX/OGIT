ogit.MARS:Machine
	a rdfs:Class;
	rdfs:subClassOf ogit:Entity;
	rdfs:label "Machine";
	dcterms:description "Vertices of this type are the 'M'-nodes in the MARS model. 'M' stand for 'Machine'. But to be precise a 'Machine' node models any operation system instance (regardless whether running on bare metal or in some virtualized environment)";
	# For ranges, see http://dublincore.org/documents/dcmi-period/
	dcterms:valid "start=2018-05-21;";
	dcterms:creator "FCO";
	dcterms:created "2015-05-15";
	dcterms:modified "2015-05-15";
	ogit:admin-contact "arago GmbH";
	ogit:tech-contact "arago GmbH";
	ogit:scope "NTO";
	ogit:parent "http://www.purl.org/ogit/MARS/Node";
	ogit:mandatory-attributes (

	);
	ogit:optional-attributes (
              ogit.MARS.Machine:cpuArch

		ogit:endOfWarranty
		ogit:serialNumber
	);
	ogit:indexed-attributes (
		ogit:name
	);
	ogit:history (
	);
