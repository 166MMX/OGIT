@prefix ogit.MARS.Network:   <http://www.purl.org/ogit/MARS/Network/> .
@prefix ogit:                   <http://www.purl.org/ogit/> .
@prefix owl:                    <http://www.w3.org/2002/07/owl#> .
@prefix rdfs:                   <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dcterms:                <http://purl.org/dc/terms/> .

ogit.MARS.Network:interfaceIP
	a owl:DatatypeProperty;
	rdfs:subPropertyOf ogit:Attribute;
	rdfs:label "interfaceIP";
	dcterms:description "Specifies the configured IP address of all defined interfaces. It's a key'ed list with the key to be defined in attribute ogit/MARS/Network/interfaceName of the same Machine node. In case of IPv4 addresses dot-decimal notation shall be used. Replaces 'NetworkInterfaceIP' from MARS-Schema";
	# For ranges, see http://dublincore.org/documents/dcmi-period/
	dcterms:valid "start=2018-06-01;";
	dcterms:creator "fotto@arago.de";
	dcterms:created "2018-05-25";
	dcterms:modified "2018-05-25";
	ogit:admin-contact "arago GmbH";
	ogit:tech-contact "arago GmbH";
.

